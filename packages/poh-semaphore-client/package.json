{
  "name": "poh-semaphore-client",
  "version": "0.1.0",
  "description": "A library for interacting this poh-semaphore based services.",
  "license": "MIT",
  "main": "dist/index.node.js",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.node.js"
  },
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "scripts": {
    "build:watch": "rollup -c rollup.config.ts -w --configPlugin typescript",
    "build": "rimraf dist && rollup -c rollup.config.ts --configPlugin typescript",
    "prepublishOnly": "yarn build",
    "docs": "typedoc src/index.ts --out ../../docs/group"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "rollup": "^2.64.0",
    "typedoc": "^0.22.11"
  },
  "dependencies": {
    "@semaphore-protocol/group": "^2.0.0",
    "@semaphore-protocol/identity": "^2.0.0",
    "@semaphore-protocol/subgraph": "^0.2.0",
    "circomlibjs": "0.0.8",
    "ethers": "^5.7.0",
    "snarkjs": "^0.4.24"
  }
}
